{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "poeticsoft/columntools",
  "title": "Column Tools",
  "category": "poeticsoft",
  "icon": "media-archive",
  "description": "Column tools",
  "keywords": [],
  "textdomain": "poeticsoft",
  "version": "1.0.0",
  "parent": [ 
    "core/column" 
  ],
  "supports": {
    "align": ["left", "center", "right"],    
    "anchor": false,
    "customClassName": true,
    "className": true,
    "html": false,
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "dimensions": {
      "minHeight": true,
      "width": true
    }
  },
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "refClientId": {
      "type": "string",
      "default": ""
    },
    "defaultOpen": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./build/editor.js",
  "editorStyle": "file:./build/editor.css",
  "viewScript": "file:./build/view.js",
  "viewStyle": "file:./build/view.css",
  "render": "file:./render.php"
}
